.stream-item
  .stream-item-dialogue
    %img(ng-src="{{userImageUrl(stream.user.image_id)}}")
    %h5
      %a(ng-href="/profiles/{{stream.user.slug}}" onclick='track_viewed_profile_through_stream();')
        {{stream.user.name}}
      voted for
      {{stream.trackable.vote.votable.title}} in the poll:
      %a(ng-href="{{stream.trackable.vote.votable.poll.path}}" onclick='track_viewed_item_through_stream();')
        {{stream.trackable.vote.votable.poll.title}}
      %small
        {{stream.created_at|date:'MMM d, yyyy @ h:mma'}}
