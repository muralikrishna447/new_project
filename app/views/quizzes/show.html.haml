.hero-unit.quiz-title
  .container
    .row
      .span12
        %h3= quiz.title

#quiz-overview
  .container
    .row
      .span12
        %h3
          Test your skills on
          %strong #{quiz.title}!
    .row.hero-unit
      .span8
        = image_tag 'http://lorempixel.com/745/376/food/', class: 'img-polaroid'
      .span4
        = progress_dial 0, 20, 'questions'
        = progress_dial 5, 15, 'est. min.'
    .row
      .span12
        %p.pull-left
          Lorem ipsum Quiz Start Text
        .pull-right
          .btn-uppercase.btn-primary.btn-x-large Start Quiz

