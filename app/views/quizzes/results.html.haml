- content_for :title, "#{quiz.title.html_safe} Results"

.hero-unit.quiz-title
  .container
    .row
      .span12
        %h3 Your Results for #{quiz.title}

#quiz-results.container
  = render 'quizzes/multiple_choice_question_results', results: quiz_results[:multiple_choice]
  = render 'quizzes/box_sort_question_results', results: quiz_results[:box_sort]

