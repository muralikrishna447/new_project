
/ %cs-image(url='image' ng-click='togglePlay()' ng-class='imageClass')
.cs-image-video-image(ng-click='togglePlay()' ng-class='imageClass')
  %img(ng-src='{{imageUrl(image)}}')
.cs-video(ng-class='videoClass')
  .embed-container
    %iframe(ng-src='{{videoUrl(video)}}' frameborder='0' allowfullscreen)