.actions
  %button.admin-button.edit Edit
  %button.admin-button.delete{ data: {confirm: "Are you sure?"} } Delete

%span.type Multiple Choice
%h4= hb('instructions')
.question-box
  %h3= hb('question')

  %ul.options
    = hb 'each options' do
      = hb 'if correct' do
        %li.option.correct= hb('answer')
      = hb 'unless correct' do
        %li.option= hb('answer')

